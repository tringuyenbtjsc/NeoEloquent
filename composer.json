{
  "name": "vinelab/neoeloquent",
  "description": "Laravel wrapper for the Neo4j graph database REST interface",
  "keywords": [
    "neo4j",
    "ogm",
    "graph",
    "database",
    "neoeloquent"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Abed Halawi",
      "email": "abed.halawi@vinelab.com"
    },
    {
      "name": "Kinane Domloje",
      "email": "kinane@vinelab.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/contracts": "^10.0||^11.0||^12.0",
    "nesbot/carbon": "^3.8",
    "laudis/neo4j-php-client": "^3.2"
  },
  "require-dev": {
    "mockery/mockery": "^1.6",
    "phpunit/phpunit": "^9.6",
    "symfony/var-dumper": "^7.2",
    "fzaninotto/faker": "^1.5",
    "composer/composer": "^2.1",
    "laravel/pint": "^1.21"
  },
  "autoload": {
    "psr-4": {
      "Vinelab\\NeoEloquent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vinelab\\NeoEloquent\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Vinelab\\NeoEloquent\\NeoEloquentServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
